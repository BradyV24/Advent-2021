          char[][] map1 = new char[5][13];
          for (int y = 0; y < mapL.size(); y++ ) {
              for (int x = 0; x < mapL.get(y).size(); x++ ) {
                  map1[y][x] = mapL.get(y).get(x);
              }
          }
          System.out.println(game1(map1, 0));

